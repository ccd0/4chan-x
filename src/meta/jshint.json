{
  "esversion": 6,
  "undef":   true,
  "unused":  true,
  "eqnull":  true,
  "expr":    true,
  "sub":     true,
  "scripturl": true,
  "multistr": true,
  "browser": true,
  "devel":   true,
  "nonstandard": true,
  "-W018": true,
  "-W084": true,
  "-W083": true,
  "-W093": true,
  "-W032": true,
  "globals": {
    "MediaError":   false,
    "Set":          false,
    "Promise":      false,
    "BroadcastChannel": false,
    "GM_info":      false,
    "cloneInto":    false,
    "XPCNativeWrapper": false,
    "unsafeWindow": false,
    "chrome":       false,
    "GM":           false<%=
  meta.grants.filter(x => !/\./.test(x)).map(x => `,\n    "${x}": false`).join('')
%><%=
  read('/tmp/declaration.js').match(/^var (.*);/)[1].split(', ').map(x => `,\n    "${x}": true`).join('')
%><%=
  read('/src/globals/globals.js').match(/^var (.*);/)[1].split(', ').map(x => `,\n    "${x}": true`).join('')
%>
  }
}
